<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <display-name>ROOT</display-name>
  <filter>
    <filter-name>loginFilter</filter-name>
    <filter-class>com.vmware.eucenablement.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loginFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>Encoding</filter-name>
    <filter-class>com.vmware.eucenablement.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>Encoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>M2avServlet</servlet-name>
    <servlet-class>com.vmware.eucenablement.m2av.servlet.M2avServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>M2avServlet</servlet-name>
    <url-pattern>/m2av</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InstalledAppStacks</servlet-name>
    <servlet-class>com.vmware.eucenablement.m2av.servlet.InstalledServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InstalledAppStacks</servlet-name>
    <url-pattern>/installed</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
</web-app>